<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="configurationPageRoot" stylesheets="@css/style.css"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ru.fullrest.mfr.plugins_configuration_utility.controller.PluginConfigurationController">
    <children>
        <VBox prefHeight="600.0" prefWidth="700.0">
            <children>
                <HBox alignment="CENTER" prefHeight="70.0" prefWidth="700.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#openScriptCreator" text="Сконфигурировать"
                                disable="true">
                            <HBox.margin>
                                <Insets right="3.0"/>
                            </HBox.margin>
                            <padding>
                                <Insets bottom="3.0"/>
                            </padding>
                        </Button>
                        <Button fx:id="acceptButton" disable="true" mnemonicParsing="false" onAction="#accept"
                                text="Применить">
                            <HBox.margin>
                                <Insets right="3.0"/>
                            </HBox.margin>
                            <padding>
                                <Insets bottom="3.0"/>
                            </padding>
                        </Button>
                        <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#close" text="Отмена">
                            <HBox.margin>
                                <Insets/>
                            </HBox.margin>
                            <padding>
                                <Insets bottom="3.0"/>
                            </padding>
                        </Button>
                    </children>
                    <VBox.margin>
                        <Insets/>
                    </VBox.margin>
                    <padding>
                        <Insets top="27.0"/>
                    </padding>
                </HBox>
                <HBox prefHeight="531.0" prefWidth="700.0">
                    <VBox.margin>
                        <Insets/>
                    </VBox.margin>
                    <children>
                        <VBox prefHeight="512.0" prefWidth="258.0">
                            <HBox.margin>
                                <Insets bottom="13.0" left="14.0" top="13.0"/>
                            </HBox.margin>
                            <children>
                                <ScrollPane hbarPolicy="NEVER" prefHeight="500.0" prefWidth="243.0">
                                    <content>
                                        <VBox fx:id="groupVBox" alignment="TOP_CENTER" prefHeight="490.0"
                                              prefWidth="242.0">
                                            <padding>
                                                <Insets right="13.0"/>
                                            </padding>
                                        </VBox>
                                    </content>
                                    <VBox.margin>
                                        <Insets bottom="10.0" right="5.0" top="3.0"/>
                                    </VBox.margin>
                                </ScrollPane>
                            </children>
                        </VBox>
                        <VBox prefHeight="531.0" prefWidth="440.0">
                            <HBox.margin>
                                <Insets left="4.0" right="14.0"/>
                            </HBox.margin>
                            <children>
                                <HBox prefHeight="177.0" prefWidth="425.0">
                                    <VBox.margin>
                                        <Insets top="6.0"/>
                                    </VBox.margin>
                                    <children>
                                        <VBox prefHeight="177.0" prefWidth="211.0">
                                            <children>
                                                <ScrollPane hbarPolicy="NEVER" prefHeight="200.0" prefWidth="211.0">
                                                    <content>
                                                        <VBox fx:id="releaseVBox" maxWidth="211.0" prefHeight="175.0">
                                                            <padding>
                                                                <Insets right="5.0"/>
                                                            </padding>
                                                        </VBox>
                                                    </content>
                                                </ScrollPane>
                                            </children>
                                        </VBox>
                                        <VBox prefHeight="177.0" prefWidth="211.0">
                                            <HBox.margin>
                                                <Insets left="4.0"/>
                                            </HBox.margin>
                                            <children>
                                                <ScrollPane hbarPolicy="NEVER" prefHeight="200.0" prefWidth="200.0">
                                                    <content>
                                                        <VBox fx:id="descriptionVBox" alignment="TOP_CENTER"
                                                              maxWidth="211.0" prefHeight="172.0"/>
                                                    </content>
                                                </ScrollPane>
                                            </children>
                                        </VBox>
                                    </children>
                                </HBox>
                                <HBox fx:id="iconHBox" prefHeight="331.0" prefWidth="425.0">
                                    <VBox.margin>
                                        <Insets top="4.0"/>
                                    </VBox.margin>
                                </HBox>
                            </children>
                        </VBox>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
